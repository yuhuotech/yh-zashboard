{
  "version": 2,
  "buildCommand": "exit 0",
  "outputDirectory": ".",
  "public": true,
  "env": {},
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400"
        }
      ]
    }
  ]
}
